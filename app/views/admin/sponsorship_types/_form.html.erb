<%= form_for [:admin, @competition, @sponsorship_type] do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :position %>
    <%= f.number_field :position, min: 1 %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
