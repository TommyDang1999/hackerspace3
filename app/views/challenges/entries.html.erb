<main class="challenge_page">
  <%= render 'challenge_entries_header' %>
  <% if @teams.any? %>
    <%= render partial: 'components/team_panel', collection: @teams, as: :team %>
  <% else %>
    <section class="teams">
      <article>
        <p>
          <strong>
            No team entries to display for this challenge.
          </strong>
        </p>
      </article>
    </section>
  <% end %>
  <%= link_to 'Back to Challenge', challenge_path(@challenge.identifier) %>
</main>
